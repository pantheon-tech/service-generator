{
  "name": "generator-typescript-library-starter",
  "version": "1.0.18",
  "description": "Generate node library boilerplate with TypeScript support",
  "homepage": "https://github.com/alexhayton/generator-typescript-library-starter",
  "author": {
    "name": "Alex Hayton",
    "url": "https://github.com/alexhayton"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "node",
    "library",
    "npm",
    "typescript",
    "starter",
    "generate",
    "yeoman-generator"
  ],
  "scripts": {
    "build": "tsc",
    "pretest": "npm run build",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/yeoman-assert": "^3.1.1",
    "@types/yeoman-generator": "^5.0.0",
    "@types/yeoman-test": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-unicorn": "^32.0.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^6.1.0"
  },
  "resolutions": {
    "yeoman-environment": "^3.2.0"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "fullname": "^4.0.1",
    "generator-license": "^5.4.0",
    "git-user-email": "^0.2.2",
    "git-user-name": "^2.0.0",
    "yeoman-generator": "^5.2.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "node_modules",
      "generators/app/templates"
    ],
    "collectCoverageFrom": [
      "generators/app/*.js"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "alexhayton/generator-typescript-library-starter",
  "license": "MIT"
}
